{
    "name": "joshbrw/laravel-module-migrations",
    "description": "Adds functionality for running migrations from other modules.",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "nwidart/laravel-modules": "2.*|3.*",
        "illuminate/support": "5.5.*|5.6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Brown",
            "email": "josh@joshbrown.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Joshbrw\\LaravelModuleMigrations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joshbrw\\LaravelModuleMigrations\\LaravelModuleMigrationsServiceProvider"
            ]
        }
    }
}
